<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.berenice.proyectohoteles.Buscanos"
    android:background="@drawable/buscanos">


    <Button
        android:id="@+id/btnweb"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:textColor="#000000"
        android:onClick="onWebButton"
        android:background="@drawable/web"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:id="@+id/btnRedondo"
        android:layout_width="50dp"
        android:layout_height="50dp"
           android:textColor="#000000"
        android:onClick="onFaceButton"
        android:background="@drawable/face"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/btninstagran"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:textColor="#000000"
        android:onClick="onInstagranButton"
        android:background="@drawable/instagram"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />
</RelativeLayout>